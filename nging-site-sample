server {
    listen 80;
    server_name localhost;
    sendfile off;
    set $no_cache "";
    if ($request_uri ~* \.gif$) {
    	set $no_cache "1";
    }
    proxy_no_cache $no_cache;
    proxy_cache_bypass $no_cache;

    location / { try_files $uri @aha; }
    location @aha {
 	include uwsgi_params;
        uwsgi_pass unix:/home/bishu/Projects/aha/aha.sock;
    }
}
